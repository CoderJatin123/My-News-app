<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/mobile_navigation"
            app:startDestination="@+id/navigation_home">

    <fragment
            android:id="@+id/navigation_home"
            android:name="com.example.mynews.ui.News.HomeFragment"
            android:label="@string/title_news"
            tools:layout="@layout/fragment_home"/>

    <fragment
            android:id="@+id/navigation_dashboard"
            android:name="com.example.mynews.ui.Search.DashboardFragment"
            android:label="@string/title_search"
            tools:layout="@layout/fragment_dashboard"/>
    <fragment
            android:id="@+id/navigation_save"
            android:name="com.example.mynews.ui.Save._List.SaveFragment"
            android:label="@string/title_save"
            tools:layout="@layout/save_fragment"/>

    <fragment
            android:id="@+id/navigation_notifications"
            android:name="com.example.mynews.ui.Setting.NotificationsFragment"
            android:label="@string/title_setting"
            tools:layout="@layout/fragment_notifications"/>
</navigation>